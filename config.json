{
  "name": "MySQL-CLI",
  "viewName": "MySQL CLI",
  "type": "executer",
  "imgUrl": "logo.png",
  "execProgram": "node",
  "main": "app.js",
  "version": "2.0.6",
  "description": "execute MySQL queries and scripts via cli.",
  "category": "DATABASE",
  "auth": {
    "authId": "connectionStringMysql",
    "params": [
      {
        "name": "conStr",
        "viewName": "Connection String",
        "type": "string"
      }
    ]
  },
  "settings": [
    {
      "name": "path",
      "viewName": "MySql Shell Executables Path",
      "type": "string"
    }
  ],
  "methods": [
    {
      "name": "executeQuery",
      "viewName": "Execute Query",
      "params": [
        {
          "name": "query",
          "viewName": "Query String",
          "type": "string",
          "required": true
        }
      ]
    },
    {
      "name": "executeSQLFile",
      "viewName": "Execute SQL File",
      "params": [
        {
          "name": "path",
          "viewName": "File Path",
          "type": "string"
        }
      ]
    },
    {
      "name": "dumpDataBase",
      "viewName": "Dump a Database",
      "params": [
        {
          "name": "dbName",
          "viewName": "Database Name",
          "type": "string",
          "required": true
        },
        {
          "name": "path",
          "viewName": "File Path",
          "type": "string",
          "required": true
        },
        {
          "name": "data",
          "viewName": "Include Data",
          "type": "boolean"
        }
      ]
    },
    {
      "name": "copyDataBase",
      "viewName": "Copy a Database",
      "params": [
        {
          "name": "dbName",
          "viewName": "Source Database Name",
          "type": "string",
          "required": true
        },
        {
          "name": "destConStr",
          "viewName": "Destination Connection String",
          "type": "string"
        },
        {
          "name": "dbNameCopy",
          "viewName": "New Database Name",
          "type": "string",
          "required": true
        },
        {
          "name": "data",
          "viewName": "Copy With Data",
          "type": "boolean"
        }
      ]
    }
  ]
}