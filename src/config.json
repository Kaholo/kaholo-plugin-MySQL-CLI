{
  "name": "MySQL-CLI",
  "type": "executer",
  "imgUrl": "MySQL.png",
  "execProgram": "node",
  "main": "app.js",
  "version": "1.2.0",
  "description": "Run sql commands and scripts on MySQL via MySQL cli.",
  "methods": [
    {
      "name": "executeQuery",
      "viewName": "Execute query",
      "params": [
        {
          "name": "QUERY",
          "viewName": "query string",
          "type": "string"
        },
        {
          "name": "CONNECTION_STRING",
          "viewName": "connection string",
          "type": "string"
        }
      ],
      "actionString": "CONNECTION_STRING -e 'QUERY'"
    },
    {
      "name": "executeSQLFile",
      "viewName": "Execute sql file",
      "params": [
        {
          "name": "PATH",
          "viewName": "file path",
          "type": "string"
        },
        {
          "name": "CONNECTION_STRING",
          "viewName": "connection string",
          "type": "string"
        }

      ],
      "actionString": "CONNECTION_STRING -e < PATH"
    },
    {
      "name":"dumpDataBase",
      "viewName":"Dump a database",
      "params":[
        {
          "name": "USERNAME",
          "viewName": "username",
          "type": "string"
        },
        {
          "name": "PASSWORD",
          "viewName": "password",
          "type": "string"
        },
        {
          "name": "HOST",
          "viewName": "host",
          "type": "string"
        },
        {
          "name": "PORT",
          "viewName": "port",
          "type": "string"
        },
        {
          "name": "DB_NAME",
          "viewName": "Database name",
          "type": "string"
        },
        {
          "name": "PATH",
          "viewName": "file path",
          "type": "string"
        }
      ]
    },
    {
      "name":"copyDataBase",
      "viewName":"Copy a database",
      "params":[
        {
          "name": "USERNAME",
          "viewName": "username",
          "type": "string"
        },
        {
          "name": "PASSWORD",
          "viewName": "password",
          "type": "string"
        },
        {
          "name": "HOST",
          "viewName": "host",
          "type": "string"
        },
        {
          "name": "PORT",
          "viewName": "port",
          "type": "string"
        },
        {
          "name": "DB_NAME",
          "viewName": "Database name",
          "type": "string"
        },
        {
          "name": "DATA",
          "viewName": "With data",
          "type": "boolean"
        }
      ]
    }
  ]
}
